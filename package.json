{
    "name": "mu-lang",
    "displayName": "Mu Lang",
    "description": "Mathematical Programing Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mu",
            "aliases": ["Mu", "mu"],
            "extensions": [".mu"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mu",
            "scopeName": "source.mu",
            "path": "./syntaxes/mu.tmLanguage.json"
        }]
    }
}